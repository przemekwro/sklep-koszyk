A;AACA,UAAA,KAAA,MAAA,EAAA;AACA,aAAA,YAAA,GAAA,EAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA,OAHA,MAGA;AACA,aAAA,MAAA,GAAA,IAAA;AACA;AACA;AARA,GAnBA;AA+BA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,QAAA,GAAA,KAAA,CAAA,OAAA,CAAA,WAAA;;AACA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA;AACA;;AACA,aAAA,IAAA;AACA,KAPA;AASA,IAAA,YATA,0BASA;AACA,aAAA,KAAA,CAAA,OAAA,CAAA,cAAA;AACA,KAXA;AAaA,IAAA,aAbA,2BAaA;AACA,UAAA,IAAA,GAAA,KAAA,CAAA,OAAA,CAAA,WAAA;;AAEA,UAAA,CAAA,IAAA,EAAA;AACA,eAAA,IAAA;AACA;;AAEA,UAAA,KAAA,YAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,eAAA,IAAA;AACA;;AACA,UAAA,OAAA,GAAA,EAAA;AACA,UAAA,kBAAA,GAAA,KAAA,YAAA,CAAA,WAAA,EAAA;;AACA,WAAA,IAAA,CAAA,IAAA,IAAA,EAAA;AACA,YAAA,aAAA,GAAA,IAAA,CAAA,CAAA,CAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,KAAA;AAAA,iBAAA,KAAA,CAAA,YAAA,CAAA,WAAA,GAAA,QAAA,CAAA,kBAAA,CAAA;AAAA,SAAA,CAAA;AACA,QAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,IAAA,CAAA,CAAA,CAAA,CAAA,IAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SAAA;AACA;;AACA,aAAA,OAAA;AACA;AA9BA,GA/BA;AA+DA,EAAA,OA/DA,qBA+DA;AACA,IAAA,KAAA,CAAA,QAAA,CAAA,kBAAA;AACA;AAjEA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <v-row id=\"top\" no-gutters align=\"center\" justify=\"end\">\r\n            <transition name=\"fade\">\r\n                <div v-show=\"search\" style=\"max-height: 36px; max-width: 200px \">\r\n                    <v-text-field :rules=\"rules\" v-model=\"search_field\" class=\"pa-0\" color=\"gray\"\r\n                                  label=\"Search\"></v-text-field>\r\n                </div>\r\n            </transition>\r\n            <v-btn @click=\"onOffSearch\" class=\"mr-3 ml-3\"\r\n                   tile icon>\r\n                <v-icon color=\"black\" large>mdi-magnify</v-icon>\r\n            </v-btn>\r\n            <router-link to=\"/cart\">\r\n                <v-btn color=\"white\" tile class=\"\" depressed>\r\n                    <v-icon large>mdi-cart-outline</v-icon>\r\n                    <div class=\"mt-5 d-flex justify-center align-center\"\r\n                         style=\"margin-left: -5px; z-index: 6; width: 20px; height:20px; border-radius: 25px; border: 1px solid\">\r\n                        <span>{{cart_counter}}</span>\r\n                    </div>\r\n                </v-btn>\r\n            </router-link>\r\n        </v-row>\r\n        <transition name=\"slide\">\r\n            <v-img id=\"picture\" class=\"mt-5\" max-width=\"100%\" alt=\"obrazek\" src=\"../assets/jeans.jpg\">\r\n                <div class=\"ma-16\">\r\n                    <h1>New arrivals</h1>\r\n                    <h3>COLLECTION 2016</h3>\r\n                    <v-btn @click=\"$vuetify.goTo('#products')\" tile> Shop here</v-btn>\r\n                </div>\r\n            </v-img>\r\n        </transition>\r\n        <transition name=\"fade\">\r\n            <v-container>\r\n\r\n                <div id=\"products\" class=\"pt-10\" v-if=\"category\">\r\n                    <div :id=\"cat.name\" v-for=\"cat in filteredItems\" :key=\"cat.name\">\r\n                        <h2>{{cat.name}}</h2>\r\n                        <div class=\"row\">\r\n                            <div v-if=\"cat.items.length==0\">\r\n                                <p>Items not found</p>\r\n                            </div>\r\n                            <div v-for=\"item in cat.items\" class=\"col-lg-3 offset-0 col-md-4 col-6 pt-0\"\r\n                                 :key=\"item.id\">\r\n                                <Product :prop_product=\"item\"></Product>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </v-container>\r\n        </transition>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import store from \"@/store\";\r\n    import Product from \"@/components/Product\";\r\n\r\n    export default {\r\n        name: 'page',\r\n\r\n        components: {\r\n            Product,\r\n        },\r\n\r\n        data() {\r\n            return {\r\n                cart_open: null,\r\n                downloaded: false,\r\n                search_field: '',\r\n                search: false,\r\n                rules: [\r\n                    value => (value && value.length >= 3) || 'Min 3 characters'\r\n                ],\r\n            }\r\n        },\r\n\r\n        methods: {\r\n            onOffSearch() {\r\n                if (this.search) {\r\n                    this.search_field = ''\r\n                    this.search = false\r\n                } else {\r\n                    this.search = true\r\n                }\r\n            },\r\n\r\n        },\r\n\r\n        computed: {\r\n            category() {\r\n                const category = store.getters.getCategory\r\n                if(category) {\r\n                    return category\r\n                }\r\n                return null\r\n            },\r\n\r\n            cart_counter() {\r\n                return store.getters.getItemsInCart;\r\n            },\r\n\r\n            filteredItems() {\r\n                let list = store.getters.getCategory;\r\n\r\n                if(!list){\r\n                    return null\r\n                }\r\n\r\n                if (this.search_field.length <= 2) {\r\n                    return list;\r\n                }\r\n                let newlist = [];\r\n                const lower_search_field = this.search_field.toLowerCase()\r\n                for (let i in list) {\r\n                    let find_elements = list[i].items.filter(items => items.product_name.toLowerCase().includes(lower_search_field));\r\n                    newlist.push({name: list[i].name, items: find_elements});\r\n                }\r\n                return newlist;\r\n            }\r\n        },\r\n        created() {\r\n            store.dispatch('downloadProducts')\r\n        },\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    a{\r\n        text-decoration: none;\r\n    }\r\n    .fade-enter-active {\r\n        transition: all .3s ease;\r\n    }\r\n\r\n    .fade-leave-active {\r\n        transition: all .3s cubic-bezier(1.0, 0.5, 0.8, 1.0);\r\n    }\r\n\r\n    .fade-enter, .fade-leave-to\r\n        /* .slide-fade-leave-active below version 2.1.8 */\r\n    {\r\n        transform: translateY(-60px);\r\n    }\r\n\r\n    #picture {\r\n        h1, h3, h4, h5, h6 {\r\n            color: white;\r\n            letter-spacing: 4px;\r\n            font-family: 'Montserrat', sans-serif;\r\n        }\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}