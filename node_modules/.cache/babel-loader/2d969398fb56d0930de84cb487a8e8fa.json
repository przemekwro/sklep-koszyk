MAAA,CAAA,MAAA,CAAA,EAAA,CAAA;AACA;AAdA,GAZA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,WAAA,QAAA,CAAA,IAAA,iBAAA,KAAA,OAAA,CAAA,YAAA,eAAA;AACA,QAAA,KAAA,EAAA,iBADA;AAEA,QAAA,QAAA,EAAA,WAFA;AAGA,QAAA,QAAA,EAAA;AAHA,OAAA;AAKA,MAAA,KAAA,CAAA,MAAA,CAAA,mBAAA,EAAA;AAAA,QAAA,OAAA,EAAA,KAAA,OAAA;AAAA,QAAA,MAAA,EAAA,KAAA;AAAA,OAAA;AACA,WAAA,OAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KAVA;AAWA,IAAA,aAXA,2BAWA;AACA,WAAA,QAAA,CAAA,IAAA,iBAAA,KAAA,OAAA,CAAA,YAAA,eAAA;AACA,QAAA,KAAA,EAAA,iBADA;AAEA,QAAA,QAAA,EAAA,WAFA;AAGA,QAAA,QAAA,EAAA;AAHA,OAAA;AAKA,MAAA,KAAA,CAAA,MAAA,CAAA,eAAA,EAAA;AAAA,QAAA,OAAA,EAAA,KAAA,OAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAAA;AACA,KAlBA;AAmBA,IAAA,eAnBA,6BAmBA;AACA,WAAA,OAAA;AACA,KArBA;AAsBA,IAAA,eAtBA,6BAsBA;AACA,UAAA,KAAA,OAAA,GAAA,CAAA,EAAA,KAAA,OAAA;AACA;AAxBA,GA7BA;AAwDA,EAAA,OAAA,EAAA;AACA,IAAA,YAAA,EAAA,sBAAA,KAAA,EAAA;AACA,wBAAA,KAAA;AACA;AAHA;AAxDA,CAAA","sourcesContent":["<template>\r\n    <div id=\"product_details\">\r\n        <v-container style=\"max-width: 1200px\" class=\"pa-0\">\r\n            <v-row no-gutters>\r\n                <MenuDetails></MenuDetails>\r\n                <v-container class=\"col-lg-10 col-md-10 col-sm-12 pt-5 pa-0 mb-0\" id=\"right\">\r\n                    <v-row id=\"top\" no-gutters align=\"center\" justify=\"end\">\r\n                        <router-link to=\"/\">\r\n                            <v-btn color=\"white\" tile depressed>\r\n                                <v-icon large>mdi-home-outline</v-icon>\r\n                            </v-btn>\r\n                        </router-link>\r\n                        <router-link to=\"/cart\">\r\n                            <v-btn color=\"white\" tile class=\"\" depressed>\r\n                                <v-icon large>mdi-cart-outline</v-icon>\r\n                                <div class=\"mt-5 d-flex justify-center align-center\"\r\n                                     style=\"margin-left: -5px; z-index: 6; width: 20px; height:20px; border-radius: 25px; border: 1px solid\">\r\n                                    <span>{{cart_counter}}</span>\r\n                                </div>\r\n                            </v-btn>\r\n                        </router-link>\r\n                    </v-row>\r\n                    <div>\r\n                        <v-row no-gutters>\r\n                            <div class=\"col-lg-6 col-md-6 d-flex justify-center justify-sm-center justify-lg-start\">\r\n                                <v-img max-width=\"400\" max-height=\"500\"\r\n                                       src=\"http://dummyimage.com/400x500.jpg/dddddd/000000\"></v-img>\r\n                            </div>\r\n                            <div class=\"col-lg-6 col-md-6 col-sm-12\">\r\n                                <div class=\"pa-5 pt-0\">\r\n                                    <h1 class=\"\">{{product.product_name}}</h1>\r\n                                    <h2>{{product.company}}</h2>\r\n                                    <h2 class=\"mt-2\">{{product.price}}</h2>\r\n                                    <div class=\"mt-16\">\r\n                                        <div class=\"row d-flex align-center justify-space-around mt-10\">\r\n                                            <div>\r\n                                                <h1>\r\n                                                    {{price|productPrice}}\r\n                                                </h1>\r\n                                            </div>\r\n                                            <div class=\"d-flex align-center justify-center\">\r\n                                                <v-btn @click=\"decreaseCounter\" icon large>\r\n                                                    <v-icon>mdi-minus</v-icon>\r\n                                                </v-btn>\r\n                                                <h1 class=\"mr-3 ml-3\">{{counter}}</h1>\r\n                                                <v-btn @click=\"increaseCounter\" icon large>\r\n                                                    <v-icon>mdi-plus</v-icon>\r\n                                                </v-btn>\r\n                                            </div>\r\n                                        </div>\r\n                                        <div class=\"row d-flex justify-center ma-10\">\r\n                                            <v-btn @click=\"addToCart\"> Add to cart</v-btn>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </v-row>\r\n\r\n                        <h3 class=\"mt-2\">{{product.description}}</h3>\r\n                    </div>\r\n                </v-container>\r\n            </v-row>\r\n        </v-container>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import store from \"@/store\";\r\n    import MenuDetails from \"@/components/MenuDetails\";\r\n\r\n    export default {\r\n        name: 'Product',\r\n        components: {\r\n            MenuDetails\r\n        },\r\n\r\n        data() {\r\n            return {\r\n                drawer: false,\r\n                counter: 1,\r\n            }\r\n        },\r\n        computed: {\r\n            price() {\r\n                console.log(this.product.price.slice(1))\r\n                let product_price = this.product.price.slice(1)\r\n                let priceFloat = parseFloat(product_price)\r\n                return (priceFloat * this.counter).toFixed(2)\r\n            },\r\n\r\n            cart_counter() {\r\n                return store.getters.getItemsInCart;\r\n            },\r\n\r\n            product() {\r\n                return this.$store.getters.getProduct(this.$route.params.id)\r\n            }\r\n        },\r\n\r\n        methods: {\r\n            addToCart() {\r\n                this.$toasted.show(`Added ${this.product.product_name} to cart`, {\r\n                    theme: \"toasted-primary\",\r\n                    position: \"top-right\",\r\n                    duration: 2000\r\n                });\r\n                store.commit('addToCartMultiply', {product: this.product, number: this.counter})\r\n                this.counter = 1\r\n                this.offDialog()\r\n            },\r\n            addToCartOnce() {\r\n                this.$toasted.show(`Added ${this.product.product_name} to cart`, {\r\n                    theme: \"toasted-primary\",\r\n                    position: \"top-right\",\r\n                    duration: 2000\r\n                });\r\n                store.commit('addToCartOnce', {product: this.product, number: 1})\r\n            },\r\n            increaseCounter() {\r\n                this.counter++\r\n            },\r\n            decreaseCounter() {\r\n                if (this.counter > 1) this.counter--\r\n            },\r\n        },\r\n\r\n        filters: {\r\n            productPrice: function (value) {\r\n                return `$${value}`\r\n            }\r\n        }\r\n\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n    #menubar {\r\n        position: sticky;\r\n        top: 0;\r\n        z-index: 7;\r\n    }\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}