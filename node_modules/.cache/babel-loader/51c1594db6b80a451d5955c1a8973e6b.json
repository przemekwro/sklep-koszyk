            </v-btn>\n        </div>\n\n    </div>\n</template>\n\n<script>\n\n    import store from \"@/store\";\n\n    export default {\n        name: 'menubar',\n\n        components: {},\n\n        methods:{\n            set_category(category){\n                store.commit('set_selected_category',category)\n            },\n            first_category(){\n                this.set_category(this.category[0].name)\n            }\n        },\n        mounted(){\n            this.first_category()\n        },\n\n        data() {\n            return {\n                category: [\n                    {\n                        name: 'Shirts',\n                        subcategory: [\n                            {name: 'Long sleeve'},\n                            {name:'T-shirt'},\n                        ],\n                    },\n                    {\n                        name: 'Dresses',\n                        subcategory: [\n                            {name: 'Summer'},\n                            {name:'Jeans'},\n                        ],\n                    },\n                    {\n                        name: 'Jeans',\n                        subcategory: [\n                            {name: 'Skinny'},\n                            {name:'Relaxed'},\n                        ],\n                    },\n                    {\n                        name: 'Jacket',\n                        subcategory: [\n                            {name: 'Summer'},\n                            {name:'Relaxed'},\n                        ],\n                    },\n                    {\n                        name: 'Gymwear',\n                        subcategory: [\n                            {name: 'Shirts'},\n                            {name:'Shorts'},\n                        ],\n                    }, {\n                        name: 'Blazers',\n                        subcategory: [\n                            {name: 'Long'},\n                            {name:'Short'},\n                        ],\n                    },\n                ],\n            }\n        },\n    }\n</script>\n\n<style lang=\"scss\">\n    #menubar {\n\n        .v-list{\n            .v-list-group--active{\n\n                .v-list-item--active{\n                    .v-icon{\n                        visibility: visible;\n                    }\n                }\n\n\n                .v-list-item__icon{\n                    visibility: visible;\n                }\n            }\n            .v-icon{\n                visibility: hidden;\n            }\n        }\n\n\n        #expand_menu {\n            .v-btn {\n                color: #757575;\n            }\n\n            .v-btn:hover {\n                background: #ccc;\n                color: black;\n            }\n        }\n\n        #footer {\n            .v-btn {\n                color: black;\n            }\n\n            .v-btn:hover {\n                background: #ccc;\n                color: black;\n            }\n        }\n    }\n</style>\n"],"sourceRoot":"src/components"}]}