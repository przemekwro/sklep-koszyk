">\r\n        <v-card class=\"pa-6\" rounded=\"0\" color=\"white\">\r\n            <h2>Your cart</h2>\r\n            <div v-if=\"cart.length ==0\">\r\n                Your cart is empty.\r\n            </div>\r\n            <div v-if=\"cart.length>0\">\r\n\r\n\r\n                    <v-row v-for=\"item in cart\" class=\"pb-5\" no-gutters :key=\"item.id\">\r\n                        <Cart_item :product=\"item\"></Cart_item>\r\n                    </v-row>\r\n\r\n                <v-row justify=\"end\" align-content=\"center\" no-gutters>\r\n                    <v-col align-self=\"center\" class=\"justify-center\">\r\n                        <div class=\"d-flex justify-center\">\r\n                            <span>All: ${{cart_price}}</span>\r\n                        </div>\r\n                    </v-col>\r\n                    <v-col>\r\n                        <div class=\"d-flex justify-center\">\r\n                            <v-btn @click=\"buy\">Buy</v-btn>\r\n                        </div>\r\n                    </v-col>\r\n                </v-row>\r\n            </div>\r\n        </v-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import store from \"@/store\";\r\n    import Cart_item from \"@/components/Cart_item\";\r\n\r\n    export default {\r\n        name: 'cart',\r\n\r\n        components: {\r\n            Cart_item\r\n        },\r\n\r\n        computed: {\r\n            cart() {\r\n                return store.getters.get_cart\r\n            },\r\n            product_to_cart() {\r\n                return {\r\n                    product: this.$props['product'],\r\n                    number: this.counter,\r\n                }\r\n            },\r\n            cart_price() {\r\n                return store.getters.get_cart_value\r\n            },\r\n        },\r\n\r\n        methods: {\r\n            add_to_cart() {\r\n                store.commit('add_to_cart', this.product_to_cart)\r\n                this.counter = 1\r\n                this.dialog = false\r\n            },\r\n            buy() {\r\n                store.commit('clear_cart')\r\n            },\r\n        },\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}