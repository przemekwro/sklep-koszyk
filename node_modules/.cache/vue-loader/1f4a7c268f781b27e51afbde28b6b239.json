>\r\n                    <v-row id=\"top\" no-gutters align=\"center\" justify=\"end\">\r\n                        <router-link to=\"/\">\r\n                            <v-btn color=\"white\" tile depressed>\r\n                                <v-icon large>mdi-home-outline</v-icon>\r\n                            </v-btn>\r\n                        </router-link>\r\n                        <router-link to=\"/cart\">\r\n                            <v-btn color=\"white\" tile class=\"\" depressed>\r\n                                <v-icon large>mdi-cart-outline</v-icon>\r\n                                <div class=\"mt-5 d-flex justify-center align-center\"\r\n                                     style=\"margin-left: -5px; z-index: 6; width: 20px; height:20px; border-radius: 25px; border: 1px solid\">\r\n                                    <span>{{cart_counter}}</span>\r\n                                </div>\r\n                            </v-btn>\r\n                        </router-link>\r\n                    </v-row>\r\n\r\n                    <div class=\"container\">\r\n                        <h2>Your cart</h2>\r\n                        <div v-if=\"cart.length ==0\">\r\n                            Your cart is empty.\r\n                        </div>\r\n                        <div v-if=\"cart.length>0\">\r\n                            <v-row v-for=\"item in cart\" class=\"pb-5\" no-gutters :key=\"item.id\">\r\n                                <Cart_item :prop_product=\"item\"></Cart_item>\r\n                            </v-row>\r\n                            <v-row justify=\"end\" align-content=\"center\" no-gutters>\r\n                                <v-col align-self=\"center\" class=\"justify-center\">\r\n                                    <div class=\"d-flex justify-center\">\r\n                                        <span>All: ${{cart_price}}</span>\r\n                                    </div>\r\n                                </v-col>\r\n                                <v-col>\r\n                                    <div class=\"d-flex justify-center\">\r\n                                        <v-btn @click=\"buy\">Buy</v-btn>\r\n                                    </div>\r\n                                </v-col>\r\n                            </v-row>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <div id=\"address\" class=\"mt-10 container\">\r\n                        <h2>Address</h2>\r\n                        <v-row>\r\n                            <v-col cols=\"4\">\r\n                                <v-text-field label=\"Name\"></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"4\">\r\n                                <v-text-field label=\"Surname\"></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"4\">\r\n                                <v-text-field label=\"Email\" :rules=\"emailRules\"></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"4\">\r\n                                <v-text-field label=\"Street\"></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"4\">\r\n                                <v-text-field label=\"Post-Code\"></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"4\">\r\n                                <v-text-field hint=\"aaa\" label=\"City\"></v-text-field>\r\n                            </v-col>\r\n                        </v-row>\r\n                    </div>\r\n\r\n                    <div id=\"payments\" class=\"mt-10 container\">\r\n                        <h2>Payments</h2>\r\n\r\n                        <v-radio-group v-model=\"payments\">\r\n                            <v-radio label=\"Credit Card\" value=\"credit_card\"></v-radio>\r\n                            <v-radio label=\"Transfer\" value=\"transfer\"></v-radio>\r\n                        </v-radio-group>\r\n                        <div v-if=\"payments==='transfer'\">\r\n                            <p>Account number: <br>12 1234 4321 1234 1234 4213 1234</p>\r\n                            <p>Title: <br>1808200012</p>\r\n                            <p>Address:<br>LOGO<br>123 Somewhere Place<br>Nothest PA 12345-4321</p>\r\n                        </div>\r\n                        <div v-else>\r\n                            <div class=\"mb-16\">\r\n                                <h2>Address</h2>\r\n                                <v-row>\r\n                                    <v-col>\r\n                                        <v-row align=\"center\">\r\n\r\n                                            <v-text-field\r\n                                                    v-model=\"month\"\r\n                                                    label=\"MM\"\r\n                                                    append-outer-icon=\"mdi-plus\"\r\n                                                    @click:append-outer=\"incrementMonth\"\r\n                                                    prepend-icon=\"mdi-minus\"\r\n                                                    @click:prepend=\"decrementMonth\"\r\n                                                    readonly\r\n                                                    style=\"max-width: 6em\">\r\n                                            </v-text-field>\r\n                                            <span class=\"ml-3 mr-3 text-h4\">/</span>\r\n                                            <v-text-field\r\n                                                    v-model=\"year\"\r\n                                                    label=\"YY\"\r\n                                                    append-outer-icon=\"mdi-plus\"\r\n                                                    @click:append-outer=\"incrementYear\"\r\n                                                    prepend-icon=\"mdi-minus\"\r\n                                                    @click:prepend=\"decrementYear\"\r\n                                                    readonly\r\n                                                    style=\"max-width: 6em\">\r\n                                            </v-text-field>\r\n                                            <br>\r\n                                            <v-text-field\r\n                                                    v-model.number=\"year\"\r\n                                                    label=\"YY\"\r\n                                                    append-outer-icon=\"mdi-plus\"\r\n                                                    @click:append-outer=\"incrementYear\"\r\n                                                    prepend-icon=\"mdi-minus\"\r\n                                                    @click:prepend=\"decrementYear\"\r\n                                                    style=\"max-width: 6em\">\r\n                                            </v-text-field>\r\n                                        </v-row>\r\n                                    </v-col>\r\n                                </v-row>\r\n                                <v-row>\r\n                                    <v-col cols=\"4\">\r\n                                        <v-text-field label=\"Surname\"></v-text-field>\r\n                                    </v-col>\r\n                                </v-row>\r\n                                <v-row>\r\n                                    <v-col cols=\"4\">\r\n                                        <v-text-field label=\"Email\" :rules=\"emailRules\"></v-text-field>\r\n                                    </v-col>\r\n                                </v-row>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </v-container>\r\n            </v-row>\r\n        </v-container>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import store from \"@/store\";\r\n    import Cart_item from \"@/components/Cart_item\";\r\n    import MenuCart from \"@/components/MenuCart\";\r\n\r\n    export default {\r\n        name: 'cart',\r\n\r\n        components: {\r\n            Cart_item,\r\n            MenuCart,\r\n        },\r\n\r\n        data() {\r\n            return {\r\n                month: 1,\r\n                year: 20,\r\n                cvv:'000',\r\n                card_validity: 'Card validity',\r\n                payments: 'credit_card',\r\n                emailRules: [\r\n                    v => !!v || 'E-mail is required',\r\n                    v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\r\n                ],\r\n            }\r\n        },\r\n\r\n        computed: {\r\n\r\n            cart() {\r\n                return store.getters.getCart\r\n            },\r\n            product_to_cart() {\r\n                return {\r\n                    product: this.$props['product'],\r\n                    number: this.counter,\r\n                }\r\n            },\r\n\r\n            cart_counter() {\r\n                return store.getters.getItemsInCart;\r\n            },\r\n\r\n            cart_price() {\r\n                return store.getters.getCartValue\r\n            },\r\n        },\r\n\r\n        methods: {\r\n            buy() {\r\n                store.commit('clearCart')\r\n            },\r\n            incrementMonth() {\r\n                if (this.month < 12)\r\n                    this.month++\r\n            },\r\n            decrementMonth() {\r\n                if (this.month > 1)\r\n                    this.month--\r\n            },\r\n            decrementYear() {\r\n                let today = new Date()\r\n                let current = today.getFullYear()\r\n                let year = `20${this.year}`\r\n                console.log(current)\r\n                if (parseInt(year) > current)\r\n                    this.year--\r\n            },\r\n            incrementYear(){\r\n                this.year++\r\n            },\r\n        },\r\n\r\n\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    .fade-enter-active {\r\n        transition: all 1s ease;\r\n    }\r\n\r\n    .fade-leave-active {\r\n        transition: all 1s cubic-bezier(1.0, 0.5, 0.8, 1.0);\r\n    }\r\n\r\n    .fade-enter, .fade-leave-to\r\n        /* .slide-fade-leave-active below version 2.1.8 */\r\n    {\r\n        opacity: 0;\r\n    }\r\n\r\n</style>\r\n"]}]}