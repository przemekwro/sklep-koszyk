            <span>{{product.product.price}}</span>\r\n        </div>\r\n        <div class=\"col-4 col-lg-3 col-md-3 col-sm-4\">\r\n            <div class=\"row d-flex align-center\">\r\n                <v-btn icon @click=\"increase_item\">\r\n                    <v-icon>mdi-plus</v-icon>\r\n                </v-btn>\r\n                <span>{{product.number}}</span>\r\n                <v-btn icon @click=\"decrease_item\">\r\n                    <v-icon>mdi-minus</v-icon>\r\n                </v-btn>\r\n            </div>\r\n        </div>\r\n        <div class=\"col-4 col-lg-2 col-md-2 col-sm-4 d-flex align-center\">\r\n            ${{price}}\r\n        </div>\r\n        <div class=\"col-4 col-lg-2 col-md-2 col-sm-4\">\r\n            <v-btn @click=\"delete_item\" icon>\r\n                <v-icon>mdi-delete</v-icon>\r\n            </v-btn>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import store from \"@/store\";\r\n\r\n    export default {\r\n        name: 'product',\r\n        props: ['product'],\r\n\r\n        data: () => ({\r\n        }),\r\n\r\n        computed: {\r\n\r\n            price() {\r\n                var product_price = this.$props['product'].product.price.slice(1)\r\n                var priceFloat = parseFloat(product_price)\r\n                return (priceFloat * this.$props['product'].number).toFixed(2)\r\n            },\r\n\r\n        },\r\n\r\n        methods: {\r\n            increase_item() {\r\n                console.log('Product ' + this.$props['product'].product.id)\r\n                store.commit('add_to_cart_once', {product: this.$props['product'].product, number: 1})\r\n            },\r\n            decrease_item() {\r\n                store.commit('decrease_number', {product: this.$props['product'].product, number: 1})\r\n            },\r\n            delete_item() {\r\n                store.commit('remove_from_cart', {product: this.$props['product'].product, number: 1})\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n    #product_card {\r\n        color: black !important;\r\n    }\r\n\r\n\r\n</style>\r\n"]}]}