    </v-btn>\r\n\r\n        <div class=\"mt-10\">\r\n            <v-btn class=\"justify-start text-capitalize font-weight-regular\"\r\n                   text tile x-large height=\"35\" width=\"100%\" to=\"#subscribe\"\r\n                   @click=\"$vuetify.goTo('#subscribe',{offset:30})\">\r\n                Subscribe\r\n            </v-btn>\r\n\r\n            <v-btn class=\"justify-start text-capitalize font-weight-regular\"\r\n                   text tile x-large height=\"35\" width=\"100%\" @click=\"newsletter = !newsletter\">\r\n                Newsletter\r\n            </v-btn>\r\n            <v-dialog id=\"newsletter\" max-width=\"800px\" v-model=\"newsletter\">\r\n                <v-card class=\"pa-7 pt-3\">\r\n                    <v-container>\r\n                        <div class=\"row\">\r\n                            <div class=\"col d-flex justify-center\"></div>\r\n                            <div class=\"col d-flex justify-center\"><h2>Newsletter</h2></div>\r\n                            <div class=\"col d-flex justify-center\">\r\n                                <v-btn @click=\"newsletter = false\" large icon>\r\n                                    <v-icon>mdi-close</v-icon>\r\n                                </v-btn>\r\n                            </div>\r\n                            <div class=\"col-12 d-flex justify-center\">Join our mailing list to receive updates on new\r\n                                arrivals and special offers.\r\n                            </div>\r\n                        </div>\r\n                        <div>\r\n                            <v-text-field label=\"Your Email\"></v-text-field>\r\n                            <div class=\"d-flex justify-center\">\r\n                                <v-btn>Subscribe</v-btn>\r\n                            </div>\r\n                        </div>\r\n                    </v-container>\r\n                </v-card>\r\n            </v-dialog>\r\n            <v-btn class=\"justify-start text-capitalize font-weight-regular\"\r\n                   text tile x-large height=\"35\" width=\"100%\" to=\"#contact\"\r\n                   @click=\"$vuetify.goTo('#contact'),{offset:10}\">\r\n                Contact\r\n            </v-btn>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\n    import store from \"@/store\";\r\n\r\n    export default {\r\n        name: 'menubar',\r\n\r\n        data() {\r\n            return {\r\n                newsletter: false,\r\n                category: [\r\n                    {\r\n                        name: 'Shirts',\r\n                        items: []\r\n                    },\r\n                    {\r\n                        name: 'Dresses',\r\n                        items: []\r\n                    },\r\n                    {\r\n                        name: 'Jeans',\r\n                        items: []\r\n                    },\r\n                    {\r\n                        name: 'Jackets',\r\n                        items: []\r\n                    },\r\n                    {\r\n                        name: 'Gymwear',\r\n                        items: []\r\n                    },\r\n                ],\r\n            }\r\n        },\r\n\r\n        methods: {\r\n            set_category(category) {\r\n                store.commit('set_selected_category', category)\r\n            },\r\n        },\r\n\r\n        mounted() {\r\n            this.set_category(this.category)\r\n        },\r\n\r\n        computed: {\r\n            cart() {\r\n                return store.getters.get_cart\r\n            }\r\n        },\r\n\r\n\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n</style>\r\n"]}]}